{
    "plugins": [
        "prettier",
        "import",
        "@typescript-eslint"
    ],
    "extends": [
        "plugin:prettier/recommended",
        "prettier",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "env": {
        "es2021": true,
        "browser": true,
        "node": true
    },
    "parser": "@typescript-eslint/parser",

    "rules": {
        "no-debugger": "off",
        "no-console": 0,
        "class-methods-use-this": "off"
    }
}
